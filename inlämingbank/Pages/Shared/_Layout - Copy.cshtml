<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="https://kit.fontawesome.com/41c8b474e2.css" crossorigin="anonymous">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/css/bootstrap.min.css" rel="stylesheet"
          integrity="sha384-GLhlTQ8iRABdZLl6O3oVMWSktQOp6b7In1Zl3/Jr59b6EGGoI1aFkw7cmDA6j6gD" crossorigin="anonymous">
    <script defer src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/js/bootstrap.bundle.min.js"
            integrity="sha384-w76AqPfDkMBDXo30jS1Sgez6pr3x5MlQ1ZAGC+nuZB+EYdgRZgiwxhTBTkF7CXvN"
            crossorigin="anonymous"></script>
    <link rel="stylesheet" href="~/css/style.css">
    <title>Bank.</title>
</head>

<body>
  
                
               
               
                
                  
                    
                
  



    <header>
        <div class="container">
            <a id="logotype" asp-area="" asp-page="/Index">Inlämingbank.</a>
            <div id="menu">
                <nav id="menu-links" class="links">
                    <a class="link" asp-area="" asp-page="/Index">Home</a>
                    <a class="link" asp-area="" asp-page="/Accounts">Accounts</a>
                    @if (User.IsInRole("Admin"))
                    {
                        <a class="link" asp-area="" asp-page="/Privacy">Privacy</a>
                        <a class="link" asp-area="Identity"
                       asp-page="/Account/Register">Register</a>
                    }
                    <partial name="_LoginPartial" />
                </nav>
                
            </div>
        </div>
    </header>








    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="">
        <div class="container">
            &copy; 2023 - inlämingbank - <a asp-area="" asp-page="/Privacy">Privacy</a>
        </div>
    </footer>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>

    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>